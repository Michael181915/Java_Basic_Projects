import java.util.Scanner;
import java.io.File;

import javax.swing.JOptionPane;

public class OperFile {
	static String [][] h = new String [10][10];
	static Scanner scc;
	
	public static void main (String args[]) { 
		openFile();
		readFile();	
		out();
	}

	private static void out() {
		while(scc.hasNext()){
			for(int r=0;r<h.length;r++) {
				for(int c=0;c<h[r].length;c++) {
					System.out.print(h[r][c]+"  ");
				}
			}
		}		
	}

	private static void readFile() {
		while(scc.hasNext()){
			for(int r=0;r<h.length;r++) {
				for(int c=0;c<h[r].length;c++) {
					h[r][c] = scc.next();
				}
			}
		}
	}

	private static void openFile() {
		try{
			scc = new Scanner(new File("res//1.txt"));
		}catch(Exception e) {JOptionPane.showMessageDialog(null,"Файл не найден");}
	}
}
